<table class="table">
  <thead>
    <tr>
      <th>氏名</th>
      <th colspan="3"></th>
    </tr>
  </thead>
  <tbody>
    <% users.each do |user| %>
      <tr>
        <td><%= user.name %></td>
        <td><%= render "public/relationships/btn", user: user %></td>
        <td><%= link_to "詳細", user, class: "user_#{user.id}" %></td>
      </tr>
    <% end %>
  </tbody>
</table>
